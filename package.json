{
  "name": "cypress-types",
  "description": "A vitejs plugin which automatically incorporates your own Cypress \"commands\" into Cypress's typing definition",
  "version": "0.1.0",
  "author": "Ken Snyder <ken@ken.net>",
  "license": "MIT",
  "main": "dist/index.mjs",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "watch": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "cypress": "^9.6.1",
    "ts-morph": "^14.0.0",
    "vite": "^2.9.9"
  },
  "devDependencies": {
    "@types/node": "16",
    "@vitest/ui": "^0.12.6",
    "tsup": "^5.12.8",
    "typescript": "^4.6.4",
    "vitest": "^0.12.6"
  }
}
